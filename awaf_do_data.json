{
    "schemaVersion": "1.3.0",
    "class": "Device",
    "label": "Basic onboarding",
    "Common": {
        "class": "Tenant",
        "hostname": "f5awaf.example.com",
        "dbvars": {
            "class": "DbVariables",
            "ui.advisory.enabled": true,
            "ui.advisory.color": "green",
            "ui.advisory.text": "/Common/hostname"
        },
        "myDns": {
            "class": "DNS",
            "nameServers": [
                "8.8.8.8"
            ],
            "search": [
                "f5.com"
            ]
        },
        "myNtp": {
            "class": "NTP",
            "servers": [
                "0.us.pool.ntp.org",
                "0.pool.ntp.org",
                "1.pool.ntp.org"
            ],
            "timezone": "UTC"
        },
        "myProvisioning": {
            "class": "Provision",
            "ltm": "nominal"
			"asm": "nominal"
        },
        "internal": {
            "class": "VLAN",
            "tag": 4094,
            "mtu": 1500,
            "interfaces": [
                {
                    "name": "1.0",
                    "tagged": false
                }
            ]
        },
        "default": {
            "class": "Route",
            "gw": "10.216.8.1",
            "network": "default",
            "mtu": 1500
        }
        
    }
}